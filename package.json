{
  "name": "taai",
  "version": "1.0.0",
  "description": "TAAI - The AI Grader integrated with Google Classroom",
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt"
  },
  "keywords": ["ai", "grading", "education", "google-classroom"],
  "author": "Teerapat Trepopsakulsin",
  "license": "GPL-3.0"
}
